    "# Handling Events"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "from tkinter import *"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "root = Tk()\n",
    "root.geometry(\"800x400\")\n",
    "def timer(event):\n",
    "    print(f\"It Clicks {event.x} , {event.y}\")\n",
    "b1 = Button(text = \"Click me please\")\n",
    "b1.pack()\n",
    "b1.bind(\"<Double-1>\" , timer)\n",
    "\n",
    "\n",
    "\n",
    "root.mainloop()"
   ]
